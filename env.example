# PropCalc Root Environment Configuration
# Copy this file to .env and fill in your actual values

# Database
POSTGRES_PASSWORD=your_secure_database_password_here

# Security
SECRET_KEY=your_super_secret_key_at_least_32_characters_long

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# Frontend
NODE_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:8000

# Sentry - DO NOT COMMIT ACTUAL DSN TO VERSION CONTROL
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ENVIRONMENT=development

# Grafana
GRAFANA_PASSWORD=your_secure_grafana_password_here

# Backend Database
DATABASE_URL=postgresql://vantage_user:your_secure_database_password_here@db:5432/vantage_ai
REDIS_URL=redis://redis:6379/0
