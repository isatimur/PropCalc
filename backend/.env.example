# PropCalc Backend Environment Configuration

# Database
DATABASE_URL=postgresql://vantage_user:vantage_password@localhost:5432/vantage_ai

# Application
APP_NAME=PropCalc Backend
VERSION=2.0.0
ENVIRONMENT=development
DEBUG=true

# API
API_PREFIX=/api
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]

# Security
SECRET_KEY=your-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Redis
REDIS_URL=redis://localhost:6379/0

# Logging
LOG_LEVEL=INFO

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# Cache
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# File Upload
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=[".csv", ".xlsx", ".json"]

# Data Processing
BATCH_SIZE=1000
MAX_WORKERS=4

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# DLD Integration
DLD_DATA_DIRECTORY=./data
DLD_UPDATE_INTERVAL=3600

# KML Integration
KML_DATA_DIRECTORY=./data
